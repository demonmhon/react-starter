{
  "name": "react-starter",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "React app",
  "license": "MIT",
  "author": "thanawat.petchsri@gmail.com",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config=config/webpack.prod.js",
    "build:serve": "npm run build && npm run start",
    "prettier": "prettier --check \"src/**/*.(js|jsx|css)\"",
    "prettier:write": "prettier --check \"src/**/*.(js|jsx|css)\" --write",
    "lint": "eslint -c .eslintrc --ext .js \"src/**/*.{js,jsx}\"",
    "dev": "cross-env NODE_ENV=development webpack serve --config=config/webpack.dev.js",
    "start": "npm run dev",
    "serve": "node server.js",
    "analyze": "webpack --config=config/webpack.prod.js --profile --json > webpack-bundle-analyzer/stats.json && webpack-bundle-analyzer webpack-bundle-analyzer/stats.json",
    "prepare": "husky install",
    "pretest": "npm run prettier",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-stage-0": "^7.8.3",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^30.0.5",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^14.3.2",
    "eslint": "^9.33.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "i18next": "^25.4.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.6.2",
    "prettier-plugin-style-order": "^0.2.2",
    "pretty-quick": "^4.2.2",
    "prop-types": "^15.8.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-i18next": "^15.7.1",
    "react-router": "^7.8.1",
    "react-router-dom": "^7.8.1",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "express": "^4.17.2"
  }
}
